#: ../src/guess.c:162
#, c-format
msgid "Here is Wonderful Guess Game!\n"
msgstr "Добро пожаловать в замечательную угадайку!\n"

#: ../src/guess.c:163
#, c-format
msgid "You can enjoy it for days. What a lovely one!\n"
msgstr "Вы можете наслаждаться ей днями напролет -- красота!\n"

#: ../src/guess.c:164
#, c-format
msgid "You can play it using arabic and roman numbers. What a diversity!\n"
msgstr "Вы можете использовать арабские и римские числа!\n"

#: ../src/guess.c:170
#, c-format
msgid "Guess the number from I to C\n"
msgstr "Загадайте число от I до C\n"

#: ../src/guess.c:172
#, c-format
msgid "Guess the number from 1 to 100\n"
msgstr "Загадайте число от 1 до 100\n"

#: ../src/guess.c:176
#, c-format
msgid "Is the number greater then %s?\n"
msgstr "Число больше %s?\n"

#: ../src/guess.c:178
#, c-format
msgid "Is the number greater then %d?\n"
msgstr "Число больше %d?\n"

#: ../src/guess.c:184
msgid "Yes"
msgstr "Да"

#: ../src/guess.c:187
msgid "No"
msgstr "Нет"

#: ../src/guess.c:191
msgid "Enter Yes or No"
msgstr "Введите Да или Нет"

#: ../src/guess.c:195
#, c-format
msgid "The number is %s\n"
msgstr "Число: %s\n"

#: ../src/guess.c:197
#, c-format
msgid "The number is %d\n"
msgstr "Число: %d\n"
